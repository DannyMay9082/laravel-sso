{
    "name": "adlyalimin/laravel-sso",
    "description": "Simple PHP SSO integration for Laravel",
    "keywords": ["laravel", "sso", "authentication", "login"],
    "homepage": "https://github.com/AdlyAlimin/laravel-sso",
    "license": "MIT",
    "authors": [
      {
        "name": "Adly Alimin",
        "email": "adlyalimin@gmail.com",
        "homepage": "https://github.com/AdlyAlimin",
        "role": "Developer"
      }
    ],
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": ">=6.3",
        "laravel/framework": ">=7.4",
        "zefy/php-simple-sso": "^1.0"
    },
    "autoload": {
        "psr-4": {
          "AdlyAlimin\\LaravelSSO\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
      "laravel": {
        "providers": [
            "AdlyAlimin\\LaravelSSO\\SSOServiceProvider"
        ]
      }
    }
}
